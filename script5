-- Services
local Players = game:GetService("Players")
local player = Players.LocalPlayer

-- ƒê·ª£i nh√¢n v·∫≠t load
local function getCharacter()
    return player.Character or player.CharacterAdded:Wait()
end

-- ƒê·ª£i Obby t·ªìn t·∫°i
local function getObby()
    return game.Workspace:WaitForChild("Map"):WaitForChild("GloveLocation"):WaitForChild("RegularMap"):WaitForChild("üóø")
end

-- Teleport
local function teleportToObby()
    local char = getCharacter()
    local hrp = char:WaitForChild("HumanoidRootPart")
    local obby = getObby()

    -- N·∫øu Obby l√† Model th√¨ l·∫•y PrimaryPart ho·∫∑c Part ƒë·∫ßu ti√™n
    local targetPart
    if obby:IsA("Model") then
        targetPart = obby.PrimaryPart or obby:FindFirstChildWhichIsA("BasePart")
    elseif obby:IsA("BasePart") then
        targetPart = obby
    end

    if targetPart then
        hrp.CFrame = targetPart.CFrame + Vector3.new(0, 5, 0)
        print("‚úÖ Teleport th√†nh c√¥ng ƒë·∫øn Obby")
    else
        warn("‚ùå Kh√¥ng t√¨m th·∫•y Part ƒë·ªÉ teleport")
    end
end

-- Ch·∫°y
teleportToObby()
